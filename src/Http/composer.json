{
    "name": "lazy-php/http",
    "description": "The Lazy PHP framework HTTP package",
    "keywords": [
        "php",
        "lazy framework",
        "psr",
        "psr-7",
        "psr-17",
        "psr-15",
        "psr-18",
        "http",
        "http-message",
        "http-factory",
        "http-server-handler",
        "http-server-middleware",
        "http-client"
    ],
    "homepage": "https://github.com/dionchaika/lazy-php",
    "version": "v1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dion Chaika",
            "email": "dionchaika@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lazy\\Http\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-client": "^1.0"
    }
}
